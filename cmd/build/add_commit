#!/bin/bash
echo "
//#################################################################
//###                                                           ###
//###   This script is autogenerated by cmd/build/add_commit    ###
//###                                                           ###
//###                        DO NOT EDIT                        ###
//###                                                           ###
//#################################################################

OmniConfig = window.OmniConfig || {}
OmniConfig.all = OmniConfig.all || {}
OmniConfig.all.commit = \"`git rev-parse --short HEAD`\";" > docs/js-development/commit.js
